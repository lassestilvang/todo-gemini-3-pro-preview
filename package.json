{
  "name": "todo-gemini-3-pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run --bun next dev",
    "build": "bun run --bun next build && bun scripts/generate-sw.ts",
    "analyze": "next experimental-analyze --port 8888",
    "start": "bun run --bun next start",
    "lint": "eslint",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:ci": "bun scripts/db-migrate-ci.ts",
    "db:seed": "bun src/db/seed.ts",
    "db:reset-dev": "neonctl branches reset dev --parent",
    "test": "bun test --max-concurrency=1",
    "test:coverage": "bun test --coverage",
    "test:coverage:check": "bun test --coverage --coverage-reporter=lcov --max-concurrency=1 && bun -e 'import { readFileSync } from \"fs\"; const lcovPath = \"coverage/lcov.info\"; const content = readFileSync(lcovPath, \"utf8\"); const records = content.split(\"end_of_record\"); const buckets = { lib: { covered: 0, total: 0, threshold: 70 }, components: { covered: 0, total: 0, threshold: 60 } }; for (const record of records) { const sfMatch = record.match(/^SF:(.+)$/m); if (!sfMatch) continue; const filePath = sfMatch[1]; const bucketKey = filePath.includes(\"/src/lib/\") ? \"lib\" : filePath.includes(\"/src/components/\") ? \"components\" : null; if (!bucketKey) continue; const bucket = buckets[bucketKey]; const daLines = record.match(/^DA:\\\\d+,\\\\d+/gm) || []; for (const line of daLines) { const [, count] = line.split(\":\")[1].split(\",\"); bucket.total += 1; if (Number(count) > 0) bucket.covered += 1; } } const failures = []; for (const [name, bucket] of Object.entries(buckets)) { if (bucket.total === 0) continue; const percent = (bucket.covered / bucket.total) * 100; const rounded = Math.round(percent * 100) / 100; if (rounded < bucket.threshold) { failures.push(`${name} coverage ${rounded}% < ${bucket.threshold}%`); } else { console.log(`${name} coverage ${rounded}% >= ${bucket.threshold}%`); } } if (failures.length > 0) { console.error(\"Coverage thresholds not met:\"); for (const failure of failures) console.error(`- ${failure}`); process.exit(1); }'",
    "test:e2e": "bunx playwright test",
    "test:e2e:ui": "bunx playwright test --ui",
    "test:e2e:headed": "bunx playwright test --headed"
  },
